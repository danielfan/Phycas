<?xml version="1.0" encoding="UTF-8"?>
<frame DefaultCloseOperation="JFrame.HIDE_ON_CLOSE" title="P H Y C A S Output" layout="BorderLayout" size="400,600" id="output" >
<menubar id="mb">
<menu text="FILE" id="menu_file" mnemonic="VK_F">
<menu text="Print" id="menu_print" mnemonic="VK_P" enabled="false"/>
<menu text="Save" id="menu_save" mnemonic="VK_S">
<menuitem Action="saveAction_Output" ActionCommand="saveAction_Output" text="Output Text" id="mi_saveOut" mnemonic="VK_O"/>
</menu>
<menu text="Close" id="menu_close" mnemonic="VK_C">
<menuitem Action="closeAction_Output" ActionCommand="closeAction_Output" text="Output Window" id="mi_close" mnemonic="VK_C"/>
</menu>
<menuitem Action="clearAction_Output" ActionCommand="clearAction_Output" text="Clear Output Text" id="mi_clear" mnemonic="VK_L"/>
<menuitem Action="exitAction" text="Exit Phycas" id="mi_exit" mnemonic="VK_E"/>
</menu>
<menu text="EDIT" id="menu_edit" mnemonic="VK_E">
<menu text="Properties" id="menu_prop" mnemonic="VK_R" enabled="false">
</menu>
</menu>
</menubar>
<tabbedpane id="tpOutput">
<panel layout="BorderLayout" id="pnl_out" name="Output">
<scrollpane>
<textarea Font="Lucida Sans Typewriter" wrapstyleword="true" linewrap="true" editable="false" text="" id="taOutput"/>
</scrollpane>
</panel>
</tabbedpane>
</frame>
