<project>
    <shortName>phycas</shortName>
    <fullName>Phycas</fullName>
    <version>1.0</version>
    <installerFilename>${product_shortname}-${product_version}-${platform_name}-installer.${platform_exec_suffix}</installerFilename>
    <readmeFile></readmeFile>
    <licenseFile>license.txt</licenseFile>
    <wrapLicenseFileText>1</wrapLicenseFileText>
    <leftImage></leftImage>
    <logoImage></logoImage>
    <splashImage>../images/phycaslogo.png</splashImage>
    <defaultLanguage>en</defaultLanguage>
    <allowLanguageSelection>0</allowLanguageSelection>
    <postInstallationScript></postInstallationScript>
    <postInstallationScriptArguments></postInstallationScriptArguments>
    <showPostInstallationScriptResult>0</showPostInstallationScriptResult>
    <preUninstallationScript></preUninstallationScript>
    <preUninstallationScriptArguments></preUninstallationScriptArguments>
    <allowedLanguages></allowedLanguages>
    <cdromDirectory>${product_shortname}-${product_version}-cdrom</cdromDirectory>
    <cdromDiskSize>700000000</cdromDiskSize>
    <cdromFirstDiskSize>650000000</cdromFirstDiskSize>
    <defaultInstallationMode></defaultInstallationMode>
    <defaultUnixDirectoryPermissions>755</defaultUnixDirectoryPermissions>
    <defaultUnixFilePermissions>644</defaultUnixFilePermissions>
    <description>Phycas is Python software for phylogenetic analysis</description>
    <outputDirectory>${installbuilder_install_root}/output</outputDirectory>
    <projectSchemaVersion>1.2</projectSchemaVersion>
    <saveRelativePaths>1</saveRelativePaths>
    <slideShowLoop>1</slideShowLoop>
    <slideShowTiming>5</slideShowTiming>
    <startMenuGroupName>${product_fullname}</startMenuGroupName>
    <style>standard</style>
    <summary>Phycas is both a script that can be used for Bayesian phylogenetic analyses as well as a library that can be used to construct your own Python programs for phylogenetic analyses.</summary>
    <uninstallerDirectory>${installdir}</uninstallerDirectory>
    <uninstallerName></uninstallerName>
    <validatorProgram></validatorProgram>
    <vendor>Phycas.org</vendor>
    <componentList>
        <component>
            <name>default</name>
            <description>Default Component</description>
            <canBeEdited>1</canBeEdited>
            <detailedDescription></detailedDescription>
            <selected>1</selected>
            <show>1</show>
            <desktopShortcutList/>
            <folderList>
                <folder>
                    <description>Program Files</description>
                    <destination>${installdir}</destination>
                    <name>programfiles</name>
                    <platforms>all</platforms>
                    <shortcutList>
                        <shortcut>
                            <comment>Uninstall</comment>
                            <exec>${installdir}/${uninstallerName}</exec>
                            <icon></icon>
                            <name>Uninstall ${product_fullname}</name>
                            <path>${installdir}</path>
                            <platforms>all</platforms>
                            <windowsExec>${installdir}/${uninstallerName}.exe</windowsExec>
                            <windowsExecArgs></windowsExecArgs>
                            <windowsIcon></windowsIcon>
                            <windowsPath>${installdir}</windowsPath>
                        </shortcut>
                    </shortcutList>
                </folder>
                <folder>
                    <description>Conversions</description>
                    <destination>${installdir}/pyphy/Conversions</destination>
                    <name>Conversions</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>Conversions/_Conversions.pyd</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Conversions/__init__.py</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>DataMatrix</description>
                    <destination>${installdir}/pyphy/DataMatrix</destination>
                    <name>DataMatrix</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>DataMatrix/_DataMatrixBase.pyd</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>DataMatrix/__init__.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>DataMatrix/_DataMatrix.py</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Likelihood</description>
                    <destination>${installdir}/pyphy/Likelihood</destination>
                    <name>Likelihood</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>Likelihood/_TreeLikelihood.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Likelihood/__init__.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Likelihood/_LikelihoodBase.pyd</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Likelihood/_MCMCChainManager.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Likelihood/_Model.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Likelihood/_QMatrix.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Likelihood/_SimData.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Likelihood/_TopoPriorCalculator.py</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Phylogeny</description>
                    <destination>${installdir}/pyphy/Phylogeny</destination>
                    <name>Phylogeny</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>Phylogeny/_TreeNode.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Phylogeny/__init__.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Phylogeny/_Phylogeny.pyd</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Phylogeny/_Tree.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>Phylogeny/_TreeManip.py</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>ProbDist</description>
                    <destination>${installdir}/pyphy/ProbDist</destination>
                    <name>ProbDist</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>ProbDist/_UniformDist.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/__init__.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/_BernoulliDist.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/_BetaDist.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/_BinomialDist.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/_DirichletDist.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/_ExponentialDist.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/_GammaDist.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/_InverseGammaDist.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/_Lot.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/_ProbDist.pyd</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ProbDist/_SliceSampler.py</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>ReadNexus</description>
                    <destination>${installdir}/pyphy/ReadNexus</destination>
                    <name>ReadNexus</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>ReadNexus/_ReadNexus.pyd</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ReadNexus/__init__.py</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>ReadNexus/_NexusReader.py</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
                <folder>
                    <description>Install Directory</description>
                    <destination>${installdir}</destination>
                    <name>Installdir</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>doctestall.py</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
            </folderList>
            <postInstallationActionList>
                <registryGet>
                    <key>HKEY_CURRENT_USER\Environment</key>
                    <name>FAKEPYTHONPATH</name>
                    <variable>fakepythonpath</variable>
                </registryGet>
                <registrySet>
                    <key>HKEY_CURRENT_USER\Environment</key>
                    <name>FAKEPYTHONPATH</name>
                    <type>REG_SZ</type>
                    <value>${installdir}</value>
                </registrySet>
            </postInstallationActionList>
            <startMenuShortcutList>
                <startMenuShortcut>
                    <comment>Uninstall ${product_fullname}</comment>
                    <name>Uninstall ${product_fullname}</name>
                    <windowsExec>${installdir}/uninstall.exe</windowsExec>
                    <windowsExecArgs></windowsExecArgs>
                    <windowsIcon></windowsIcon>
                    <windowsPath>${installdir}/</windowsPath>
                </startMenuShortcut>
            </startMenuShortcutList>
        </component>
    </componentList>
    <customLanguageFileList/>
    <fileList/>
    <finalPageActionList/>
    <parameterList>
        <infoParameter>
            <name>CheckPython</name>
            <title>Python Check</title>
            <description></description>
            <explanation>You must have Python 2.4 (or a later version) installed before you can use Phycas.</explanation>
            <value>${pythonversion}</value>
            <default></default>
            <leftImage></leftImage>
            <trimMarginNumChars>0</trimMarginNumChars>
        </infoParameter>
        <directoryParameter>
            <name>installdir</name>
            <title></title>
            <description>Installer.Parameter.installdir.description</description>
            <explanation>Installer.Parameter.installdir.explanation</explanation>
            <value>${platform_install_prefix}/${product_shortname}-${product_version}</value>
            <default>${platform_install_prefix}/${product_shortname}-${product_version}</default>
            <allowEmptyValue>0</allowEmptyValue>
            <ask>yes</ask>
            <cliOptionName>prefix</cliOptionName>
            <leftImage></leftImage>
            <mustBeWritable>yes</mustBeWritable>
            <mustExist>0</mustExist>
            <width>40</width>
        </directoryParameter>
    </parameterList>
    <platformOptionsList>
        <platformOptions>
            <splashImage>../images/phycaslogo.png</splashImage>
            <platform>windows</platform>
        </platformOptions>
    </platformOptionsList>
    <postInstallationActionList/>
    <postUninstallationActionList/>
    <preBuildActionList/>
    <preInstallationActionList>
        <setInstallerVariableFromScriptOutput>
            <exec>python</exec>
            <execArgs>-c 'import sys; print sys.version'</execArgs>
            <name>pythonversion</name>
        </setInstallerVariableFromScriptOutput>
        <registryGet>
            <key>HKEY_CURRENT_USER\Environment</key>
            <name>PYTHONPATH</name>
            <variable>pythonpath</variable>
        </registryGet>
    </preInstallationActionList>
    <preUninstallationActionList>
        <registryDelete>
            <key>HKEY_CURRENT_USER\Environment</key>
            <name>FAKEPYTHONPATH</name>
        </registryDelete>
    </preUninstallationActionList>
    <slideShowImageList/>
</project>

